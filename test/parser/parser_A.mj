// level A syntax test
program level_a_syntax_test
    const int ca = 1, cb = 6;
    const char cc = 's';
    const bool cd = true;

    int ga, gb[];
    char gc, gd[];
    bool ge, gf[];
{
    void main()
        int la, lb[];
        char lc, ld[];
        bool le, lf[];
    {
        gb = new int[5];
        gd = new char[2];
        gf = new bool[6];
        lb = new int[4];
        ld = new char[2];
        lf = new bool[8];

        ga = ca + cb * 5 - (15 - 9);
        ga++;
        print(ga, 5);
        gb[4] = 6 % 2 + (27 * (5 - 7));
        read(gc);
        gd[1] = 'a';
        read(gd[0]);
        ge = false;
        gf[0] = cd;
        gf[3] = gf[0];
        read(gf[2]);
        print(gf[3]);

        la = ga *= 5;
        la--;
        lb[2] = 8 * la - 4 *= -4 + 6 * 2;
        lc = gc;
        ld[1] = 'F';
        le = false;
        lf[2] = cd;
    }
}